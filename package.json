    },
    "devDependencies": {
        "babel-eslint": "*",
        "babel-plugin-transform-es2015-modules-commonjs": "*",
        "babel-plugin-transform-class-properties": "*",
        "babel-plugin-transform-decorators-legacy": "*",
        "babel-preset-react": "*",
        "electron-prebuilt": "0.37.4",
        "electron-packager": "*",
        "eslint-plugin-react": "*",
