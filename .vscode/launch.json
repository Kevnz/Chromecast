{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch the App",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/app/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/app",
            "preLaunchTask": "build:ui",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "runtimeArgs": [],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}